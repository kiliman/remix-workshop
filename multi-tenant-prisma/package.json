{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "cross-env NODE_ENV=development npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development remix dev",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "start": "remix-serve build",
    "docker": "docker-compose up -d",
    "xsetup": "prisma generate && prisma migrate deploy && prisma db seed",
    "setup": "prisma/setup"
  },
  "dependencies": {
    "@prisma/client": "^4.6.1",
    "@remix-run/node": "^1.7.5",
    "@remix-run/react": "^1.7.5",
    "@remix-run/serve": "^1.7.5",
    "bcryptjs": "^2.4.3",
    "isbot": "^3.6.5",
    "prisma": "^4.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.5",
    "@remix-run/eslint-config": "^1.7.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "cross-env": "^7.0.3",
    "eslint": "^8.27.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
}
